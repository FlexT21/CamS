{
    "ruff.interpreter": [
        ".venv/Scripts/python.exe",
        ".venv/bin/python"
    ],
    "ruff.path": [
        ".venv/Scripts/ruff.exe",
        ".venv/bin/ruff"
    ],
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        }
    }
}
